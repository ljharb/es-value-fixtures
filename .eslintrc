{
	"root": true,

	"extends": "@ljharb",

	"rules": {
		"array-bracket-newline": 0,
		"id-length": 0,
		"max-statements-per-line": 0,
		"no-magic-numbers": 0,
		"sort-keys": 0,
	},

	"overrides": [
		{
			"files": "test/**",
			"rules": {
				"func-style": 0,
				"max-lines-per-function": 0,
				"new-cap": 0,
			},
		},
	],
}

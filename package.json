{
	"name": "es-value-fixtures",
	"version": "1.0.0",
	"description": "Fixtures of ES values, for testing purposes",
	"main": "index.js",
	"exports": {
		".": [
			{
				"default": "./index.js"
			},
			"./index.js"
		],
		"./package.json": "./package.json"
	},
	"scripts": {
		"lint": "eslint --ext=.js,.mjs .",
		"pretest": "npm run lint",
		"tests-only": "tape 'test/*'",
		"test": "npm run tests-only",
		"coverage": "nyc npm run tests-only",
		"posttest": "aud --production",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ljharb/es-value-fixtures.git"
	},
	"keywords": [
		"javascript",
		"ecmascript",
		"es",
		"js",
		"values",
		"value",
		"fixture",
		"fixtures",
		"es-abstract"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"funding": {
		"url": "https://github.com/sponsors/ljharb"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ljharb/es-value-fixtures/issues"
	},
	"homepage": "https://github.com/ljharb/es-value-fixtures#readme",
	"devDependencies": {
		"@ljharb/eslint-config": "^17.2.0",
		"aud": "^1.1.2",
		"auto-changelog": "^2.2.1",
		"eslint": "^7.12.1",
		"foreach": "^2.0.5",
		"isarray": "^2.0.5",
		"nyc": "^10.3.2",
		"object-keys": "^1.1.1",
		"tape": "^5.0.1"
	},
	"dependencies": {
		"has-bigints": "^1.0.0",
		"has-symbols": "^1.0.1",
		"object.assign": "^4.1.1"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true
	}
}
